<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Thời khóa biểu</title>
    <style>
        div {
            color: black;
        }
    </style>
    <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/table-to-json@1.0.0/lib/jquery.tabletojson.min.js" integrity="sha256-H8xrCe0tZFi/C2CgxkmiGksqVaxhW0PFcUKZJZo1yNU=" crossorigin="anonymous"></script>
</head>

<body>

    <select name="sweets" multiple="multiple">
  <option>CTK41</option>
  <option>CTK42</option>
  <option>CTK43</option>
  <option>CTK39</option>
  <option>CTK40</option>
  <option>DLK41</option>
</select>
    <div></div>

    <script>
        $("select").change(function() {
            var str = "";
            $("select option:selected").each(function() {
                str += $(this).text() + " ";
            });
            $("div").text(str);

            function Schedula(str) {
                const url = ` http://qlgd.dlu.edu.vn/public/DrawingClassStudentSchedules_Mau2?YearStudy=2019-2020&TermID=HK02&Week=21&ClassStudentID=${str}&t=0.4292294353406918`;
                return new Promise(resolve => {
                    tabletojson.convertUrl(url, {
                        useFirstRowForHeadings: true
                    }, (tablesAsJson) => {
                        var result = tablesAsJson[0];
                        console.log(str);
                        resolve(result);
                    });
                });
            }
            Schedula(str);
        }).change();
    </script>

</body>

</html>